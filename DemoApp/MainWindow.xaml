<Window x:Class="DemoApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoApp"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:pt="http://propertytools.org/wpf" 
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="525">

    <DockPanel>
        <TabControl  >
            <TabItem Header="Gaussian Process">
                <local:GaussianProcessUserControl DataContext="{Binding }"/>

            </TabItem>
            <TabItem Header="Kalmam Filter">

                <local:KalmanFilterUserControl DataContext="{Binding}"/>
            </TabItem>

            <TabItem Header="Particle Filter">

                <local:ParticleFilterUserControl DataContext="{Binding}"/>
            </TabItem>
        </TabControl>

        


 

    </DockPanel>
</Window>
